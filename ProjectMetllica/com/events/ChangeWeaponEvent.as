package  com.events{		import flash.events.*;	import flash.display.BitmapData;		public class ChangeWeaponEvent extends Event{				public static const CHANGE:String = "changeWeapon";		private var weaponName:BitmapData;		public function ChangeWeaponEvent(type:String,weaponName:BitmapData, bubbles:Boolean=false, cancelable:Boolean=true) {						super(type,bubbles,cancelable);			this.weaponName = weaponName;		}		public function getWeaponName():BitmapData		{			return weaponName;		}		override public function clone():Event		{			return new ChangeWeaponEvent(type,weaponName,bubbles,cancelable);		}	}	}