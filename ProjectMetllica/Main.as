package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.system.System;	import flash.utils.getTimer;	import com.Utils.StageBoundery;	import com.Utils.StatusBox;	import com.PlayerModel.PlayerModel;		public class Main extends MovieClip {				var statusBox = new StatusBox(stage);		var ben:PlayerModel;				//FPS Counter		var fps:int = 0;		var frames:int=0;		var prevTimer:Number=0;		var curTimer:Number=0;				public function Main() {						init();		}		private function init():void		{			//Character			ben = new PlayerModel(stage,stage.stageWidth/2,stage.stageHeight/2);			addChild(ben);						addChild(statusBox);			stage.addEventListener(Event.ENTER_FRAME,onEnter,false,0,true);		}		private function onEnter(evt:Event):void{			//FPS			frames+=1;			curTimer=getTimer();			if(curTimer-prevTimer>=1000)			{				fps = frames*1000/(curTimer-prevTimer);				prevTimer=curTimer;				frames=0;			}												//Status Box text update			statusBox.text = "FPS: "+fps+"\nMem: "+(System.totalMemoryNumber/100000);		}	}	}